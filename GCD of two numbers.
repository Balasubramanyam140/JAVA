          class Main{
              public static void main(String[] args){
              int num1 =3;
              int num2 = 8;
              int num =1;
              while(num2%num1!=0){
                  num = num2%num1; // n= 8%3=2  //n=
                  num2=num1;  // n2 = 3
                  num1=num; //n1 = 2
              }
              System.out.println(num1);
              }
          }
